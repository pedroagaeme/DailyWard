{
  "expo": {
    "name": "DailyWard",
    "slug": "DailyWard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dailyward-logo.png",
    "scheme": "DailyWard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dailyward-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pedroagaeme.DailyWard"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@zoontek/react-native-navigation-bar",
        {
          "android": {
            "enforceNavigationBarContrast": false
          }
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "89302489-7105-45c7-a32d-b717372e8e12"
      }
    }
  }
}
